<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to 'Посмотреть', test_path(test) %></td>
  <td><%= link_to 'Изменить', edit_test_path(test) %></td>
  <td><%= link_to 'Удалить',
                  test_path(test),
                  method: :delete,
                  data: { confirm: 'Вы уверены?'} %>
  </td>
</tr>
